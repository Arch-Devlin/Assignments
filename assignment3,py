import csv
import random

def main():
    
    colour1 = input("What is your favourate colour?")
    colour2 = input("Pick another colour")
    name1 = input("Pick a name")
    name2 = input("Pick a different name")
    name3 = input("Pick a different name again")
    adjective1 = input("Pick an adjective")
    adjective2 = input("Pick a different adjective")
    adjective3 = input("Pick another adjective")
    object1 = input("Pick an object")
    object2 = input("Pick a different object")
    object3 = input("Pick another object")
    verb1 = input("Pick a verb")
    verb2 = input("Pick a different verb")
    verb3 = input("Pick another verb")
    
